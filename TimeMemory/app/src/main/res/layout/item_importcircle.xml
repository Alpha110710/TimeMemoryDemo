<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    >
    <RelativeLayout
        android:layout_width="@dimen/x55"
        android:layout_height="@dimen/y53"
        android:layout_marginLeft="@dimen/common_14dp"
        android:layout_marginRight="@dimen/common_16dp"
        android:layout_marginTop="@dimen/common_14dp"
        android:background="@drawable/memory_bg_other">

        <com.time.memory.gui.nineGridImage.NineGridImageView
            android:id="@id/time_imsg"
            android:layout_width="@dimen/x35"
            android:layout_height="@dimen/x35"
            android:layout_below="@id/time_title_tv"
            android:layout_marginLeft="@dimen/common_7dp"
            android:layout_marginTop="@dimen/common_26dp"
            app:imgGap="0dp"
            app:maxSize="4"
            app:showStyle="fill"
            />
    </RelativeLayout>

    <TextView
        android:id="@id/memory_desc_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="@dimen/common_10dp"
        android:layout_marginTop="@dimen/common_10dp"
        android:text="高三九班"
        android:textColor="@color/grey_48"

        />

    <ImageView
        android:id="@id/memory_sign_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="@dimen/common_14dp"
        android:src="@drawable/nosee_checkbox"
        />

</LinearLayout>

